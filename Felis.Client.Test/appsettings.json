{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "FelisClient": {
    "Router": {
      "Endpoint": "https://localhost:7103"
    },
    "Service": {
      "Name": "name",
      "Host": "host",
      "IsPublic": true,
      "Topics": []
    },
    "RetryPolicy": {
      "Attempts": 5
    },
    "Cache": {
      "SlidingExpiration": 3600,
      "AbsoluteExpiration": 3600,
      "MaxSizeBytes": 3000
    }
  }
}
